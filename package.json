{
  "name": "color-palette-maker-react",
  "version": "1.0.0",
  "description": "Color Palette Maker - React SPA for extracting color palettes from images",
  "keywords": [
    "color",
    "palette",
    "react",
    "image"
  ],
  "homepage": "https://github.com/sbecker11/color-palette-maker-react#readme",
  "bugs": {
    "url": "https://github.com/sbecker11/color-palette-maker-react/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sbecker11/color-palette-maker-react.git"
  },
  "license": "MIT",
  "author": "sbecker11",
  "type": "commonjs",
  "main": "server.js",
  "scripts": {
    "start": "node scripts/run-with-venv.js node server.js",
    "build": "cd client && npm run build",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "node scripts/run-with-venv.js nodemon server.js",
    "dev:client": "cd client && npm run dev",
    "test": "cd client && npm run test",
    "test:coverage": "cd client && npm run test:coverage",
    "build:with-coverage": "npm run build && npm run test:coverage"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "color-diff": "^1.4.0",
    "express": "^4.19.2",
    "get-pixels": "^1.1.3",
    "multer": "^2.0.2",
    "node-kmeans": "^1.1.9",
    "node-vibrant": "^4.0.3",
    "quantize": "^1.0.2",
    "sharp": "^0.33.4"
  },
  "devDependencies": {
    "concurrently": "^9.1.0",
    "nodemon": "^3.1.9"
  }
}
